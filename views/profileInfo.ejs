<div class="container">
  <div class="jumbotron">
    <h3 class="text-primary text-center">
      <span class="fa fa-user"></span> Profile Information
    </h3>
    <div class="row">
      <div class="col-sm-6">
        <div class="d-flex flex-column">
          <div class="d-flex gap-2">
            <p class="p-2">Google ID</p>
            <p class="p-2"><%= user.googleId %></p>
          </div>
          <div class="d-flex gap-2">
            <p class="p-2">Display Name</p>
            <p class="p-2"><%= user.username %></p>
          </div>
          <div class="d-flex gap-2">
            <p class="p-2">Email Address</p>
            <p class="p-2"><%= user.email %></p>
          </div>
          <div class="d-flex gap-2">
            <p class="p-2">Verified</p>
            <p class="p-2"><%= user.verified %></p>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
